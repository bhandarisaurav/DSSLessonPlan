        android:gradientRadius="100"
        android:centerX="35%"
        android:centerY="50%"
        android:centerColor="#FF9305"
        android:endColor="#FF9305"
        android:startColor="#FF9305"
        android:type="radial" />
    <padding
        android:left="0dp"
        android:top="0dp"
        android:right="0dp"
        android:bottom="0dp" />
